{
    "name": "monyxie/laravel-easemob",
    "description": "环信即时通讯laravel包开发，用于环信用户、群、聊天室等功能",
    "keywords": [
        "link1st",
        "laravel",
        "easemob"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "5.7.*|5.8.*|6.*",
        "illuminate/support": "5.7.*|5.8.*|6.*",
        "ext-curl": "*",
        "psr/simple-cache": "^1.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "authors": [
        {
            "name": "link1st",
            "email": "link12stl@gmail.com"
        },
        {
            "name": "monyxie",
            "email": "monyxie@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "link1st\\Easemob\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "link1st\\Easemob\\EasemobServiceProvider"
            ]
        }
    },
    "replace": {
        "link1st/laravel-easemob": "0.1.0"
    }
}
